    "name": "Ultimate Cipher Encryptor",
    "version": "1.0",
    "description": "Encrypt and decrypt messages with Ultimate Cipher and send them via WhatsApp Web.",
    "permissions": ["scripting", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
